						Part20.Position = Vector3.new(100.96167, 23.2699795, -108.967567)
						Part20.Rotation = Vector3.new(0.50999999, -36.5, 0.349999994)
						Part20.Color = Color3.new(0.105882, 0.164706, 0.207843)
